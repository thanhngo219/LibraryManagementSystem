      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="223.0" prefWidth="473.0">
         <children>
            <Label layoutX="45.0" layoutY="79.0" text="ID" />
            <Label layoutX="209.0" layoutY="34.0" text="Login" />
            <Label layoutX="45.0" layoutY="122.0" text="Password" />
            <TextField fx:id="txtID" layoutX="135.0" layoutY="75.0" prefHeight="25.0" prefWidth="288.0" />
            <Button fx:id="btnSubmit" layoutX="192.0" layoutY="160.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="27.0" prefWidth="90.0" text="Submit" />
            <PasswordField fx:id="txtPassword" layoutX="137.0" layoutY="117.0" prefHeight="27.0" prefWidth="288.0" />
         </children>
      </AnchorPane>
   </children>
