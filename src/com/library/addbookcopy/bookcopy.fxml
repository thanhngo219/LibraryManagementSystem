<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" prefHeight="199.0" prefWidth="365.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.library.addbookcopy.BookCopyController">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="ISBN Number" GridPane.rowIndex="1" />
      <TextField fx:id="txtISBN" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button fx:id="btnLookUp" mnemonicParsing="false" onAction="#lookUp" prefHeight="27.0" prefWidth="83.0" text="Lookup" GridPane.rowIndex="2" />
      <Button fx:id="btnAddBookCopy" disable="true" mnemonicParsing="false" onAction="#addBookCopy" prefHeight="27.0" prefWidth="117.0" text="Add BookCopy" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Add Book Copy" GridPane.columnSpan="2" />
   </children>
</GridPane>
